{
  "name": "my-qwik-basic-starter",
  "description": "App with Routing built-in (recommended)",
  "engines": {
    "node": ">=15.0.0"
  },
  "private": true,
  "scripts": {
    "build.client": "cross-env NODE_ENV=production vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "cross-env NODE_ENV=production vite build -c adapters/express/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "echo 'Run \"npm run qwik add\" to install a server adapter'",
    "serve": "cross-env NODE_ENV=production node server/entry.express",
    "dev": " cross-env NODE_ENV=local vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "preview": "qwik build preview && vite preview --open",
    "start": "cross-env THEME=theme-0 vite --open --mode ssr",
    "qwik": "qwik",
    "qwik-speak-extract": "qwik-speak-extract --supportedLangs=en,id,cn --assetsPath=i18n"
  },
  "devDependencies": {
    "@builder.io/partytown": "^0.8.1",
    "@builder.io/qwik": "1.3.2",
    "@builder.io/qwik-city": "1.3.2",
    "@types/compression": "^1.7.5",
    "@types/eslint": "8.44.9",
    "@types/express": "4.17.21",
    "@types/node": "^20.10.8",
    "@types/node-fetch": "^2.6.10",
    "@types/set-cookie-parser": "^2.4.7",
    "@typescript-eslint/eslint-plugin": "6.13.2",
    "@typescript-eslint/parser": "6.13.2",
    "autoprefixer": "10.4.16",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "eslint-plugin-qwik": "~1.3.2",
    "express": "4.18.2",
    "node-fetch": "3.3.2",
    "postcss": "^8.4.33",
    "prettier": "3.1.1",
    "qwik-speak": "^0.18.0",
    "sass": "^1.69.7",
    "tailwindcss": "^3.3.7",
    "typescript": "5.3.3",
    "undici": "^5.28.2",
    "vite": "^5.1.5",
    "vite-tsconfig-paths": "4.2.3"
  },
  "dependencies": {
    "@easepick/core": "^1.2.1",
    "@easepick/preset-plugin": "^1.2.1",
    "@easepick/range-plugin": "^1.2.1",
    "@rive-app/canvas": "^2.7.9",
    "@splidejs/splide": "^4.1.4",
    "@splidejs/splide-extension-grid": "^0.4.1",
    "@thednp/dll": "^1.0.8",
    "date-fns": "^2.30.0",
    "kute.js": "^2.2.4",
    "motion": "^10.16.4",
    "pusher-js": "^8.3.0",
    "set-cookie-parser": "^2.6.0",
    "spicr": "github:natsl05164/spicr",
    "tinygesture": "^3.0.0"
  }
}
